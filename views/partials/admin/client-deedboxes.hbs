<table class="ui center aligned unstackable short scrolling very basic table" id="clickableTable">
  <thead>
    <tr>
      <th>Deed Box</th>
      <th colspan="2">Security</th>
      <th></th>
      <th></th>
    </tr>
  </thead>
  {{#each deedBoxes}}
  <tbody>
    <tr id="clickableRow-{{@index}}">
      <td id="clickableRow-href-{{@index}}">
        {{_id}}
      </td>
      <td colspan="2">
        {{#each securities}}
          {{this.address1}}, <br>
          {{this.address2}}, <br>
          {{this.address3}}, <br>
          {{this.eircode}}, <br>
          {{this.county}} 
        {{/each}}
      </td>
      <td>

      </td>
      <td>
        <a href='{{../clientId}}/deedBox/{{_id}}'>
          <i class="bordered grey  link folder icon"></i>
        </a>
      </td>
    </tr>
  </tbody>
  {{/each}}
</table>


<script>
  clickableTable({{deedBoxes.length}}, '{{clientId}}/deedBox')
</script>